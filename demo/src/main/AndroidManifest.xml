<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fahamutech.kdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kemifracore">

        <activity
            android:name="com.fahamutech.kcore.user.activities.MasomoActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fahamutech.kcore.user.activities.CategoryContent"
            android:label="@string/title_activity_category_content"
            android:parentActivityName="com.fahamutech.kcore.user.activities.MasomoActivity" />
        <activity
            android:name="com.fahamutech.kcore.user.activities.ReadActivity"
            android:label="@string/title_activity_read"
            android:parentActivityName="com.fahamutech.kcore.user.activities.MasomoActivity" />
        <activity android:name="com.fahamutech.kcore.user.activities.SimpleImageViewer" />

        <activity
            android:name="com.fahamutech.kcore.admin.activities.CategoryActivity"
            android:label="@string/title_activity_category"
            android:parentActivityName="com.fahamutech.kcore.admin.activities.JifunzeAdminActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.fahamutech.kcore.admin.activities.NewArticleActivity"
            android:label="@string/title_activity_testimony"
            android:parentActivityName="com.fahamutech.kcore.admin.activities.JifunzeAdminActivity"
            android:theme="@style/AppTheme" />

    </application>


</manifest>